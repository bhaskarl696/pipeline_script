            steps {
                echo 'Building'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
         stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Test') {
            steps {
                echo 'Tessting'
            }
        }
        stage('Release') {
            steps {
                echo 'Production Release'
            }
        }
    }
}
